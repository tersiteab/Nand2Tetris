/**
 * 16-bit bitwise And:
 * for i = 0..15: out[i] = (a[i] and b[i])
 */

CHIP And16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    // Put your code here:
    //Nand(a=a[16],b=b[16],out=nandout[16]);
    //Nand(a=nandout[16],b=nandout[16],out=out[16]);
    And(a=a[15],b=b[15],out=out[15]);
}